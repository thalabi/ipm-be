<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="
    	http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

	<!-- Tables -->
    <changeSet id="inst_mutual_fund_table" author="thalabi">
    	<createSequence sequenceName="inst_mutual_fund_seq"/>
        <createTable tableName="inst_mutual_fund">
            <column name="id" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="lk" type="varchar(255)"> 
                <constraints nullable="false"/> 
            </column> 
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>

			<column name="instrument_id" type="bigint"/>
            <column name="company" type="varchar(255)"/> 
        </createTable>
        <addForeignKeyConstraint baseTableName="inst_mutual_fund" baseColumnNames="instrument_id" constraintName="inst_mutual_fund_fk_1" referencedTableName="instrument" referencedColumnNames="id"/> 
    </changeSet>

</databaseChangeLog>
