<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="
		http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<!-- Tables -->
    <changeSet id="exchange_rate_table" author="thalabi">
    	<createSequence sequenceName="exchange_rate_seq"/>
        <createTable tableName="exchange_rate">
            <column name="id" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="lk" type="varchar(255)"> 
                <constraints nullable="false"/> 
            </column> 
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="as_of_date" type="timestamp(3) with time zone"/> 
            <column name="from_currency" type="varchar(3)"/>
            <column name="to_currency" type="varchar(3)"/>
            <column name="rate" type="decimal(10,4)"/> 
        </createTable>
    </changeSet>

</databaseChangeLog>
